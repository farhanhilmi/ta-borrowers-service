{
    "name": "ta-borrowers-service",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "jest": {
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "verbose": true,
        "setupFilesAfterEnv": [
            "<rootDir>/src/tests/setup/setupTest.js"
        ]
    },
    "scripts": {
        "test": "jest --coverage --detectOpenHandles",
        "start": "set NODE_ENV=production& node ./src/server.js",
        "dev": "set NODE_ENV=development&& nodemon ./src/server.js",
        "build": "babel src -d dist --ignore ./node_modules,./src/tests",
        "build-key": "babel serviceAccountKey.js -d serviceAccountKeyDis.js --ignore ./node_modules,./src/tests",
        "gcp-change": "gcloud config set project ta-borrowers-service",
        "deploy": "gcloud app deploy"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "amqplib": "^0.10.3",
        "axios": "^1.3.6",
        "busboy": "^1.6.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "firebase-admin": "^11.8.0",
        "lodash": "^4.17.21",
        "mongoose": "^7.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "underscore": "^1.13.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.8",
        "@babel/node": "^7.20.7",
        "@babel/plugin-transform-modules-commonjs": "^7.21.5",
        "@babel/preset-env": "^7.21.4",
        "babel-jest": "^29.5.0",
        "jest": "^29.5.0",
        "mongodb-memory-server": "^8.12.2",
        "nodemon": "^2.0.22"
    }
}
